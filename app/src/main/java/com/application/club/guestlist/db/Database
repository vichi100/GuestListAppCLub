#### Table Details ####


clubsdetails

name
city
location
address
lat long
imageurl


CREATE TABLE clubdetails (clubname VARCHAR(50), location VARCHAR(50),
address VARCHAR(200), imageURL VARCHAR(200), latlong VARCHAR(60));

tickets details
name
type - pass guestlist table Date

CREATE TABLE ticketdetails (clubname VARCHAR(50), type VARCHAR(50), category VARCHAR(20),
 cost VARCHAR(10), details VARCHAR(200), Day VARCHAR(15), date VARCHAR(15), totaltickets VARCHAR(3), availbletickets VARCHAR(3));


booking details

cutomername
mobile
clubname
ticket id
ticket type
cost
time

CREATE TABLE bookingdetails (cutomername VARCHAR(50), mobile VARCHAR(20), customerId VARCHAR(200), clubname VARCHAR(50),
clubid VARCHAR(50),
 QRnumber VARCHAR(200), tickettype VARCHAR(20), eventDate VARCHAR(30), cost VARCHAR(10),
 ticketDetails VARCHAR(200), bookingdate VARCHAR(30), bookingtime VARCHAR(30));


event details
clubname
dj
music
date
imageurl

CREATE TABLE eventdetails (clubid VARCHAR(50), clubname VARCHAR(50),
 djname VARCHAR(50), music VARCHAR(200), date VARCHAR(200), imageURL VARCHAR(3));


 CREATE TABLE customer (cutomername VARCHAR(50),
  mobile VARCHAR(20), customerId VARCHAR(200));


offer: clubid, clubname, offerid, offerdetails, timetoexpire, priority, DJ, music , date, imageURL


CREATE TABLE offers (clubid VARCHAR(50), clubname VARCHAR(50), city VARCHAR(50), location VARCHAR(200), offerid VARCHAR(50),
offerName VARCHAR(50), offerdetails VARCHAR(500),
 djname VARCHAR(400), music VARCHAR(200), date VARCHAR(200), starttime VARCHAR(50), imageURL VARCHAR(200), timetoexpire VARCHAR(50),
 priority VARCHAR(3));

